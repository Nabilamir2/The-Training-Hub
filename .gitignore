# Wordpress - ignore core, configuration, examples, uploads and logs.
# https://github.com/github/gitignore/blob/main/WordPress.gitignore
.user.ini
wp-content/object-cache.php
### WordPress template

# ignore everything in the "wp-content" directory, except:
# "mu-plugins", "plugins", "themes" directory
/wp-content/*
!wp-content/mu-plugins.hold/
!wp-content/plugins/
!wp-content/themes/
!wp-content/uploads/
!wp-content/languages/
!/wp-content/updraft/

/wp-content/updraft/*
!/wp-content/updraft/*.gz
/wp-content/uploads/

# ignore these plugins
/wp-content/plugins/hello.php
/wp-content/plugins/all-in-one-wp-migration/
/wp-content/plugins/all-in-one-wp-migration-unlimited-extension/
/wp-content/plugins/akismet/

# ignore specific themes
wp-content/themes/twenty*/

/wp-content/themes/doubleten/woocommerce.css
/wp-content/themes/doubleten/woocommerce.css.map
/wp-content/themes/doubleten/vendor/

# ignore node dependency directories
node_modules/

# ignore log files and databases
*.log
*.sql
*.sqlite
*.idea
*.vscode
*.zip
*.rar

/local-xdebuginfo.php
/wp-config.php
/.htaccess
/.htaccess.bk
/wordfence-waf.php
/.htaccess.bk
/.user.ini
/.maintenance
/.wp-cli
